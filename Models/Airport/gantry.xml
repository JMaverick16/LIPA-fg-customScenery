              <max>
                <pow>
                  <cos><property>/sim/time/sun-angle-rad</property></cos>
                  <value>0.15</value>
                </pow>
                <value>0</value>
              </max>
              <value>1.0</value>
            </product>
            <value>-1</value>
          </sum>
        </product>
      </expression>
	  <offset>0.4</offset>
    </dim-factor>
 </animation>
 
 <animation>
  <type>dist-scale</type>
  <object-name>acollbloom</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>1</dep>
   </entry>
   <entry>
    <ind>150</ind>
    <dep>1</dep>
   </entry>
      <entry>
    <ind>500</ind>
    <dep>2</dep>
   </entry>
   <entry>
    <ind>1500</ind>
    <dep>4.5</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.3258</x-m>
   <y-m>-1.4</y-m>
   <z-m>15.3416</z-m>
  </center>
 </animation>

 <animation>
   <type>material</type>
   <object-name>anticoll.on</object-name>
   <object-name>acollbloom</object-name>
   <emission>
    <red>1</red>
    <green>1</green>
    <blue>1</blue>
   </emission>
  </animation>
  
 <animation>
   <type>material</type>
   <object-name>lenses.on</object-name>
   <emission>
    <red>0.85</red>
    <green>0.95</green>
    <blue>0.5</blue>
   </emission>
  </animation>
  
 
 <animation>
  <type>blend</type>
  <object-name>acollbloom</object-name>
      <expression>
        <product>
          <value>-1</value>
          <sum>
            <product>
              <max>
                <pow>
                  <cos><property>/sim/time/sun-angle-rad</property></cos>
                  <value>-0.3</value>
                </pow>
                <value>-1.0</value>
              </max>
              <value>-1.0</value>
            </product>
            <value>-1</value>
          </sum>
        </product>
      </expression>
 </animation>
 
  
  <animation>
   <type>select</type>
   <object-name>volume.1</object-name>
   <object-name>volume.2</object-name>
   <object-name>volume.3</object-name>
   <object-name>volume.4</object-name>
   <object-name>volume.5</object-name>
    <condition>
      <and>
        <property>/sim/rendering/rembrandt/enabled</property>
        <greater-than>
          <property>/sim/rendering/shaders/lights</property>
          <value>0</value>
        </greater-than>
		<property>/scenery/EGOD/lighting/power</property>
		<property>/scenery/EGOD/lighting/lighting-up-time</property>
      </and>
    </condition>
  </animation>
 
  <animation>
   <type>select</type>
   <object-name>anticoll.on</object-name>
   <object-name>acollbloom</object-name>
   <condition>
    <property>/scenery/EGOD/lighting/power</property>
   </condition>
  </animation>
  
  <animation>
   <type>select</type>
   <object-name>lenses.on</object-name>
   <condition>
    <property>/scenery/EGOD/lighting/power</property>
    <property>/scenery/EGOD/lighting/lighting-up-time</property>
   </condition>
  </animation>
  
  <animation>
   <type>select</type>
   <object-name>anticoll.off</object-name>
   <object-name>lenses.off</object-name>
   <condition>
	<or>
	 <not>
	  <property>/scenery/EGOD/lighting/lighting-up-time</property>
	 </not>
	 <not>
	  <property>/scenery/EGOD/lighting/power</property>
	 </not>
	 <and>
	  <not><property>/scenery/EGOD/lighting/lighting-up-time</property></not>
	  <not><property>/scenery/EGOD/lighting/power</property></not>
	 </and>
	</or>
   </condition>
  </animation>
 
</PropertyList>