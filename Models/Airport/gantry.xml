<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>

 <path>gantry.ac</path>
 
  <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>7500</max-m>
 </animation>
 
  <effect>
   <inherits-from>Effects/model-transparent</inherits-from>
   <condition>
    <property>/sim/rendering/rembrandt/enabled</property>
   </condition>
   <object-name>acollbloom</object-name>
  </effect>
 
   <animation>
    	<type>light</type>
		<light-type>spot</light-type>
		<object-name>volume.1</object-name>
		<nopreview/>
		<direction><x>-0.9</x><y>0</y><z>-0.5</z></direction>
		<position><x>-0.777</x><y>0</y><z>14.55</z></position>
		<ambient><r>0.8</r><g>0.8</g><b>0.7</b><a>1</a></ambient>
		<diffuse><r>0.85</r><g>0.95</g><b>0.7</b><a>1</a></diffuse>
		<specular><r>0.95</r><g>0.95</g><b>0.85</b><a>1</a></specular>
		<attenuation><c>0.5</c><l>0.07</l><q>0</q></attenuation>
		<exponent>20.0</exponent>
		<cutoff>250</cutoff>
		<near-m>0.1</near-m>
		<far-m>200</far-m>
		<dim-factor>
        <expression>
        <product>
          <value>-1</value>
          <sum>
            <product>
              <max>
                <pow>
                  <cos><property>/sim/time/sun-angle-rad</property></cos>
                  <value>0.3</value>
                </pow>
                <value>0</value>
              </max>
              <value>1.0</value>
            </product>
            <value>-1</value>
          </sum>
        </product>
      </expression>
    </dim-factor>
 </animation>

