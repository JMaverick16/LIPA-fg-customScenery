<?xml version="1.0"?>

<PropertyList>

 <path>beacon-mil-nt.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>

 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>GreenFlash.1</object-name>
  <object-name>GreenFlash.2</object-name>
  <object-name>WhiteFlash1.1</object-name>
  <object-name>WhiteFlash1.2</object-name>
  <object-name>WhiteFlash2.1</object-name>
  <object-name>WhiteFlash2.2</object-name>
  <object-name>GreenHalo</object-name>
  <object-name>WhiteHalo1</object-name>
  <object-name>WhiteHalo2</object-name>
  <condition>
   <greater-than>
    <property>/sim/time/sun-angle-rad</property>
    <value>1.57</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>range</type>
  <enable-hot type="bool">false</enable-hot>
  <object-name>GreenHalo</object-name>
  <object-name>WhiteHalo1</object-name>
  <object-name>WhiteHalo2</object-name>
  <min-m>0</min-m>
  <max-m>750</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>GreenHalo</object-name>
  <object-name>WhiteHalo1</object-name>
  <object-name>WhiteHalo2</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>60.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Head</object-name>
  <object-name>GreenLight</object-name>
  <object-name>WhiteLight1</object-name>
  <object-name>WhiteLight2</object-name>
  <min-m>0</min-m>
  <max-m>1500</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Head</object-name>
  <object-name>GreenLight</object-name>
  <object-name>WhiteLight1</object-name>
  <object-name>WhiteLight2</object-name>
  <condition>
   <greater-than>
    <property>/sim/time/sun-angle-rad</property>
    <value>1.57</value>
   </greater-than>
  </condition>
  <property>/sim/time/elapsed-sec</property>
  <factor>60.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Stand</object-name>
  <condition>
   <less-than-equals>
    <property>/sim/time/sun-angle-rad</property>
    <value>1.57</value>
   </less-than-equals>
  </condition>
  <min-m>0</min-m>
  <max-m>1500</max-m>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Stand</object-name>
  <condition>
   <greater-than>
    <property>/sim/time/sun-angle-rad</property>
    <value>1.57</value>
   </greater-than>
  </condition>
  <min-m>0</min-m>
  <max-m>500</max-m>
 </animation>

 <animation>
  <type>range</type>
  <object-name>GreenFlash.1</object-name>
  <min-m>0</min-m>
  <max-m>1000</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>GreenFlash.1</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>60.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
 <animation>
  <type>flash</type>
  <object-name>GreenFlash.1</object-name>
  <center>
   <x-m>0</x-m>
   <y-m>-0.35</y-m>
   <z-m>0.7</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0.1</z>
  </axis>
  <power>100</power>
 </animation>

