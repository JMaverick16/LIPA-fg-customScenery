<?xml version="1.0"?>

<PropertyList>
    
    <path>external-spot-light.ac</path>
    
    <animation>
        <type>noshadow</type>
        <object-name>cover-mirror (Meshed)</object-name>
        <object-name>mirror (Meshed)</object-name>
        <object-name>cover-black (Meshed)</object-name>
        <object-name>lamp (Meshed)</object-name>
        <object-name>support (Meshed)</object-name>
        <object-name>axis-y (Meshed)</object-name>
        <object-name>axis-x (Meshed)</object-name>
        <object-name>mirror-bottom (Meshed)</object-name>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>cover-mirror (Meshed)</object-name>
        <object-name>axis-y (Meshed)</object-name>
        <object-name>axis-x (Meshed)</object-name>
        <diffuse>
            <red>0.7</red>
            <green>0.7</green>
            <blue>0.8</blue>
        </diffuse>
        <ambient>
            <red>0.7</red>
            <green>0.7</green>
            <blue>0.8</blue>
        </ambient>
        <specular>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </specular>
        <emission>
            <red>0.1</red>
            <green>0.0.7</green>
            <blue>0.01</blue>
            <factor-prop>sim/scenery/shelter_type_01/scene_light_factor</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>mirror (Meshed)</object-name>
        <diffuse>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </specular>
        <emission>
            <red>0.55</red>
            <green>0.35</green>
            <blue>0.05</blue>
            <factor-prop>sim/scenery/shelter_type_01/scene_light_factor</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>lamp (Meshed)</object-name>
        <diffuse>
            <red>0.9</red>
            <green>0.9</green>
            <blue>0.8</blue>
        </diffuse>
        <ambient>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </specular>
        <emission>
            <red>1.10</red>
            <green>0.9</green>
            <blue>0.8</blue>
            <factor-prop>sim/scenery/shelter_type_01/scene_light_factor</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>mirror-bottom (Meshed)</object-name>
        <diffuse>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </specular>
        <emission>
            <red>0.80</red>
            <green>0.70</green>
            <blue>0.50</blue>
            <factor-prop>sim/scenery/shelter_type_01/scene_light_factor</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>mirror-bottom (Meshed)</object-name>
        <diffuse>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </specular>
        <emission>
            <red>0.80</red>
            <green>0.70</green>
            <blue>0.50</blue>
            <factor-prop>sim/scenery/shelter_type_01/scene_light_factor</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>cover-black (Meshed)</object-name>
        <diffuse>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </diffuse>
        <ambient>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </ambient>
        <specular>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.5</blue>
        </specular>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>support (Meshed)</object-name>
        <diffuse>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </diffuse>
        <ambient>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </ambient>
        <specular>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </specular>
    </animation>
    
    <animation>
        <object-name>cover-mirror (Meshed)</object-name>
        <object-name>mirror (Meshed)</object-name>
        <object-name>cover-black (Meshed)</object-name>
        <object-name>lamp (Meshed)</object-name>
        <object-name>mirror-bottom (Meshed)</object-name>
        <type>rotate</type>
        <property>surface-positions/left-aileron-pos-norm</property>
        <factor>1</factor>
        <offset-deg>30</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.214</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList>
